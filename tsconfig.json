{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "#apis/*": [
        "./src/apis/*"
      ],
      "#assets/*": [
        "./src/assets/*"
      ],
      "#constants/*": [
        "./src/constants/*"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#containers/*": [
        "./src/containers/*"
      ],
      "#styles/*": [
        "./src/styles/*"
      ],
      "#types/*": [
        "./src/types/*"
      ]
    },
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "strictNullChecks": true
  },
  "include": [
    "src",
    "src/**/*",
    "@types/**/*.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}